#!/bin/sh
if [ -z "$1" ]; then
  # Run with default number of threads
  cat ../data/finpar-format/input.data ../data/finpar-format/output.data | ./Orig_COpenMp/GenPricing
else
  export OMP_NUM_THREADS=$1
  cat ../data/finpar-format/input.data ../data/finpar-format/output.data | ./Orig_COpenMp/GenPricing
fi




